{
  "name": "webscrapper-ui",
  "version": "0.0.0",
  "engines": {
    "node": "17.9.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .ts --ext .tsx --ignore-path .gitignore --max-warnings 0 src",
    "lint:fix": "eslint --fix --ext .ts --ext .tsx --ignore-path .gitignore src",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check ./src",
    "format": "prettier --write ./src",
    "pre-commit": "npm run format && npm run check-format && npm run lint:fix && npm run lint && npm run check-types && npm run build"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.0",
    "@mui/material": "^5.6.0",
    "@reduxjs/toolkit": "^1.8.1",
    "node-sass": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.8",
    "sass-loader": "^12.4.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.15",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "@vitejs/plugin-react": "^1.0.7",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "eslint": "^8.15.0",
    "prettier": "^2.5.1",
    "path": "^0.12.7",
    "sass": "^1.49.7",
    "typescript": "^4.5.4",
    "vite": "^2.7.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0"
  }
}
